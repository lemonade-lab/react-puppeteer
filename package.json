{
  "name": "react-puppeteer",
  "version": "1.0.0-rc.6",
  "author": "ningmengchongshui",
  "description": "截图React组件",
  "main": "./index.js",
  "type": "module",
  "scripts": {
    "dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm dev.ts",
    "build": "rollup --config rollup.config.js",
    "format": "prettier --write .",
    "check-format": "git diff --exit-code",
    "prepare": "husky"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "koa": "^2.15.3",
    "koa-mount": "^4.0.0",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/koa": "^2.15.0",
    "@types/koa-mount": "^4.0.5",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/node": "^20.3.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.19",
    "core-js": "^3.37.1",
    "husky": "^9.0.11",
    "prettier": "^3.0.3",
    "puppeteer": "^22.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.16.4",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "files": [
    "index.js",
    "types",
    ".puppeteerrc.cjs",
    "main.css",
    "react-puppeteer-env.d.ts"
  ],
  "types": "types",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./types/index.d.ts"
    },
    "./.puppeteerrc": {
      "require": "./.puppeteerrc.cjs"
    },
    "./env": {
      "types": "./react-puppeteer-env.d.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
